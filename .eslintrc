{
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "ecmaFeatures": {
    "jsx": false,
    "modules": true,
    "restParams": true,
    "spread": true
  },
  "env": {
    "browser": true,
    "jquery": true,
    "node": false
  },
  "parser": "babel-eslint",
  "rules": {
    "no-underscore-dangle": 0,
    "quotes": [2, "single"],
    "strict": [2, "never"],
    "semi": [2, "always"],
    "spaced-comment": 2,
    "object-curly-spacing": [2, "always"],
    "indent": [2, 2, {"SwitchCase": 1}],
    "no-multiple-empty-lines": [1, {"max": 1}],
    "no-debugger": 2,
    "no-console": 2,
    "no-warning-comments": 1,
    "no-unneeded-ternary": 2,
    "no-empty": 2,
    "radix": 2,
    "brace-style": 2,
    "camelcase": 1,
    "max-depth": [2, 10],
    "max-statements": [1, 30],
    "complexity": [1, 10]
  },
  "plugins": [
  ]
}
